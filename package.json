{
  "name": "olaf",
  "version": "1.0.0",
  "description": "A simple TypeScript framework based upon OpenLayers that quickly allows you to develop OL app",
  "main": "./lib/olaf.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "prod": "webpack --mode production --config webpack.prod.js",
    "build": "webpack --mode development --config webpack.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sorion/olaf.git"
  },
  "keywords": [
    "openlayers",
    "typescript"
  ],
  "author": "Sorion",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sorion/olaf/issues"
  },
  "homepage": "https://github.com/Sorion/olaf#readme",
  "devDependencies": {
    "@types/ol": "^6.4.1",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ol": "^6.4.3",
    "prettier": "^2.1.2",
    "ts-loader": "^8.0.5",
    "typescript": "^4.0.3",
    "webpack": "^5.1.2",
    "webpack-cli": "^4.0.0"
  },
  "dependencies": {}
}
